      // Windows Platform SDK constants
      CONSTANT
      	GWL_EXSTYLE          = -20
      	WS_EX_LAYERED        = 0x00080000
      	LWA_COLORKEY         = 0x00000001
      	LWA_ALPHA            = 0x00000002
      	WM_WINDOWPOSCHANGING = 0x0046
      END
      
      // Structure returned by the Windows SKD that gives information about the window
      STWINDOWPOS is Structure
      	nHWnd is int
      	nHWndInsertAfter is int
      	x is int
      	y is int
      	cx is int
      	cy is int
      	nFlags is unsigned int	
      END
      
      // Sound level
      gnVolumeLevel is real
      gnMaxNbLevel  is int
      
      // Magnetism
      gbWithoutMagnet   is boolean
      
      // Implement the event to manage the move of the window
      Event(OnMove, “*.*”, WM_WINDOWPOSCHANGING)
      
   -
