     code : |1-
      
      // Position the window
      MyWindow..Width = 500
      MyWindow..X = (Val(ExtractString(SysScreenResolution(),1)) / 2) - (MyWindow..Width / 2)
      MyWindow..Y = (Val(ExtractString(SysScreenResolution(),2)) / 2) - (MyWindow..Height / 2)
      
      // Initialization
      gnVolumeLevel = MCIVolume(“WAV”)
      gnMaxNbLevel  = Abs(IMG_Sound..Width / 39) + 1
      
      // The window is still visible
      WinStatus(AboveAll)
      
      // Defines the background color as Magenta (RGB(255,0,255))
      MyWindow..BrushColor = RGB(255,0,255)
      
      // Change the style of the window into WS_EX_LAYERED (transparent window)
      API(“user32”, “SetWindowLongA”, Handle(),GWL_EXSTYLE, WS_EX_LAYERED)
      // Specify the transparency color that is magenta and define the opacity (255)
      API(“user32”, “SetLayeredWindowAttributes”, Handle(), RGB(255,0,255), 255, LWA_COLORKEY + LWA_ALPHA)	
      
      // Group of buttons for moving & closing
      GR_GRIPCLOSING..Visible = False
      
      // Start the functions used to refresh the display
      SoundTimer()
      DisplayButton()
      
      // Invisible window at startup to avoid the blinking effect during its initialization
      MyWindow..Visible = True
