      // Summary: Displays the button used to close the application or to move it
      // Syntax:
      //DisplayButton ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // DisplayButton()
      //// Automatic procedure:
      // The procedure is run manually, during a call in the code
      // It will be repeated in loop, with 10 hundredths of a second between each call
      // Each following call runs the procedure once, without timer
      // 
      
      Procedure DisplayButton()
      
      // Variables
      X, Y are system int
      
      // Initialization
      X = LoWord(CursorPos(cpScreen))
      Y = HiWord(CursorPos(cpScreen))
      
      IF MyWindow..X <= X <= MyWindow..Width + MyWindow..X _AND_ MyWindow..Y <= Y <= MyWindow..Height + MyWindow..Y THEN
      	IF NOT GR_GRIPCLOSING..Visible THEN
      		GR_GRIPCLOSING..Visible = True	
      	END
      ELSE
      	IF GR_GRIPCLOSING..Visible THEN
      		GR_GRIPCLOSING..Visible = False	
      	END 		
      END
     type : 458752
   -
     name : DisplaySoundLevel
     procedure_id : 1219398252507717944
     type_code : 14
     code : |1-
      // Summary: Displays the current sound level of the system
      // Syntax:
      //DisplaySoundLevel ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // DisplaySoundLevel()
      //
      Procedure DisplaySoundLevel()
      
      // Variable
      X       is int
      nValue is real
      
      // Initialization
      X = 0
      gnVolumeLevel = MCIVolume(“WAV”)
      
      // Calculate the ratio for the scale of our volume
      nValue = 65535 / gnMaxNbLevel
      gnVolumeLevel = gnVolumeLevel / nValue
      
      dStartDrawing(IMG_Sound, dErase)
      
      FOR I = 1 _TO_ Abs(gnVolumeLevel)
      	dRectangle(X,0,X+15,IMG_Sound..Height,LightCyan)
      	X += 40
      END
     type : 458752
   -
     name : OnMove
     procedure_id : 1219398252507783480
     type_code : 14
     code : |1+
      // Summary: Procedure called whenever the position of the window changes
      // Syntax:
      //OnMove ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // Event(OnMove, "*.*", WM_WINDOWPOSCHANGING )
      //
      Procedure OnMove()
      
      // Variables
      bMod		 is boolean
      stWinPos is STWINDOWPOS
      
      // Retrieves the information about the move
      Transfer(&stWinPos, _EVE.lParam, Dimension(stWinPos))
      
      // If the management of the magnetism is enabled
      IF gbWithoutMagnet = False THEN
      	// Magnetize on the left border of the screen
      	IF MyWindow..X <= 50 THEN	
      		stWinPos:x = 0
      		bMod = True
      	END
      	
      	// Magnetize on the top border of the screen
      	IF MyWindow..Y <= 50 THEN		
      		stWinPos:y = 0		
      		bMod = True
      	END
      	
      	// Magnetize on the right border of the screen
      	IF MyWindow..X + MyWindow..Width >= SysXRes() - 50 THEN		
      		stWinPos:x = SysXRes() - MyWindow..Width		
      		bMod = True
      	END
      	
      	// Magnetize on the bottom border of the screen
      	IF MyWindow..Y + MyWindow..Height >= SysYRes() - 50 THEN	
      		stWinPos:y = SysYRes() - MyWindow..Height	
      		bMod = True
      	END
      	
      	// If modifications have been made
      	IF bMod = True THEN 
      		// Transmit the next coordinates
      		Transfer(_EVE.lParam, &stWinPos, Dimension(stWinPos))
      	END
      ELSE
      	// If we are positioned outside the "borders" of the screen, resume the management of magnetism
      	IF MyWindow..X > 50 _AND_ MyWindow..Y > 50 _AND_ MyWindow..X + MyWindow..Width < SysXRes() - 50 _AND_ MyWindow..Y + MyWindow..Height < SysYRes() - 50 THEN
      		gbWithoutMagnet = False
      	END
      END
     type : 458752
   -
     name : ButtonDown
     procedure_id : 1219398252507849016
     type_code : 14
     code : |1-
      // Summary: Procedure called when the left button is pressed and when it is used to check the position of the window
      // and to act consequently for the magnetism.
      // Syntax:
      //ButtonDown ()
      //
      // Parameters:
      //	None
      // Return value:
      // 	None
      //
      // Example:
      // ButtonDown()
      //
      Procedure ButtonDown()
      
      gbWithoutMagnet = False
      
      IF MyWindow..X <= 50 THEN
      	gbWithoutMagnet = True
      END
      
      IF MyWindow..Y <= 50 THEN
      	gbWithoutMagnet = True
      END
      
      IF MyWindow..X + MyWindow..Width >= SysXRes() - 50 THEN
      	gbWithoutMagnet = True
      END
      
      IF MyWindow..Y + MyWindow..Height >= SysYRes() - 50 THEN
      	gbWithoutMagnet = True
      END
     type : 458752
